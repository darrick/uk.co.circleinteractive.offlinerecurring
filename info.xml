<?xml version="1.0"?>
<extension key="uk.co.circleinteractive.offlinerecurring" type="module">
  <file>offlinerecurring</file>
  <name>Offline Recurring Payment</name>
  <description>Allows offline recurring contributions to be added to a contact via the contact edit screen</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Circle Interactive(Originally coded by Rajesh)</author>
    <email>info(at)circle-interactive.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://civicrm.org/extensions/offline-recurring-payment</url>
    <url desc="Documentation">http://civicrm.org/blogs/andyw/offline-recurring-payment-extension</url>
    <url desc="Support">https://github.com/circleinteractive/uk.co.circleinteractive.offlinerecurring/issues</url>
    <url desc="Licensing">http://civicrm.org/licensing</url>
  </urls>
  <releaseDate>2018-07-01</releaseDate>
  <version>1.7</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/OfflineRecurring</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_OfflineRecurring_Upgrader</upgrader>
</extension>
